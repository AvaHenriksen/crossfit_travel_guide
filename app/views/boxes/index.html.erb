<div class="page-header">
  <h1>
    Boxes
    <a href="/boxes/new" class="btn btn-lg btn-success">New Box</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>City</th>
        <th>Details</th>
        <th>Actions</th>
      </tr>

      <% @boxes.each do |box| %>
      <tr>
        <td><%= box.city.created_at %></td>
        <td><%= box.details %></td>
        <td>
          <a href="/boxes/<%= box.id %>" class="btn btn-primary">Show</a>
          <a href="/boxes/<%= box.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_box/<%= box.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
